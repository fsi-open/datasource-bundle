{
    "name": "fsi/datasource-bundle",
    "type": "symfony-bundle",
    "description": "FSi DataSource Bundle",
    "keywords": ["fsi", "symfony", "datasource", "datasource-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "doctrine/orm": "^2.3",
        "fsi/datasource": "^2.0",
        "symfony/framework-bundle" : "^3.4|^4.0",
        "symfony/options-resolver": "^3.4|^4.0",
        "symfony/dependency-injection" : "^3.4|^4.0",
        "symfony/doctrine-bridge": "^3.4|^4.0",
        "symfony/security-csrf": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "twig/twig": "^2.11"
    },
    "require-dev": {
        "symfony/form" : "^3.4|^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/twig-bridge" : "^3.4|^4.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DataSourceBundle\\": "" }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev",
            "2.0": "2.0-dev",
            "1.2": "1.2-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
